        toggleLogin()\r\n      } catch(err){\r\n        console.error(err)\r\n      }\r\n    }\r\n  }\r\n  \r\n\r\n  const updateQuery = newValue => dispatch({type: ACTIONS.UPDATE_QUERY, payload: newValue})\r\n\r\n  return (\r\n    <GeneralContext.Provider value={{\r\n      data: state.data,\r\n      query: state.query,\r\n      isLoading: state.isLoading,\r\n      toggleLogin,\r\n      fetchBooks,\r\n      updateQuery\r\n    }}>\r\n      {props.children}\r\n    </GeneralContext.Provider>\r\n  )\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"5f8b84088ff527d2d128cf0e3f75ee08","cacheData":{"env":{}}}